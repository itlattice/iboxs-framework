{
    "name": "iboxs/iboxs",
    "description": "the new iboxsphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "iboxsphp",
        "ORM"
    ],
    "homepage": "https://lyweb.com.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "itlattice",
            "email": "zqu1016@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "iboxs/framework": "^6.0.0",
        "iboxs/iboxs-orm": "^2.0",
        "iboxs/iboxs-filesystem": "^2.0",
        "iboxs/iboxs-multi-app": "^1.0",
        "iboxs/iboxs-view": "^1.0",
        "iboxs/iboxs-captcha": "^3.0",
        "qcloudsms/qcloudsms_php": "^0.1.4",
        "qcloud/cos-sdk-v5": "^2.6",
        "php-amqplib/php-amqplib": "^2.8",
        "phpmailer/phpmailer": "^6.7",
        "iboxs/redis": "^1.0",
        "iboxs/basic": "^1.1",
        "iboxs/payment": "^1.2",
        "iboxs/carbon": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "iboxs/iboxs-trace":"^1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        },
        "psr-0": {
            "": "extend/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php iboxs service:discover",
            "@php iboxs vendor:publish"
        ]
    }
}
