{
    "name": "topiboxs/iboxs",
    "description": "the new iboxsphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "iboxsphp",
        "ORM"
    ],
    "homepage": "http://iboxsphp.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        },
        {
            "name": "yunwuxin",
            "email": "448901948@qq.com"
        }        
    ],
    "require": {
        "php": ">=7.1.0",
        "topiboxs/framework": "^6.0.0",
        "topiboxs/iboxs-orm": "^2.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "topiboxs/iboxs-trace":"^1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        },
        "psr-0": {
            "": "extend/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php iboxs service:discover",
            "@php iboxs vendor:publish"
        ]
    }
}
